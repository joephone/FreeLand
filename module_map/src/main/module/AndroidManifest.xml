<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.transcendence.map">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 使用定位功能所需权限 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_map"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_map"
        android:supportsRtl="true"
        android:theme="@style/Theme.Freeland">

        <activity
            android:name="com.transcendence.map.main.MapIndexKt"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.transcendence.map.main.MapMainActivity"
            android:launchMode="singleTask"
            />

        <activity android:name="com.transcendence.map.gps.GSPDBAc"
            android:launchMode="singleTask"
            />


    </application>

</manifest>